{
  "version": 2,
  "builds": [
    {
      
      "src": "server.js",
      
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      
      "src": "/(.*)",
      
      "dest": "server.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Headers": "Content-Type, Authorization, X-Requested-With"
      }
    }
  ]
}